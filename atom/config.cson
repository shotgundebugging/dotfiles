"*":
  "aligner-ruby": {}
  "atom-ctags": {}
  "atom-package-deps":
    ignored: []
  "autocomplete-plus":
    autoActivationDelay: 250
    backspaceTriggersAutocomplete: true
    maxVisibleSuggestions: 5
    strictMatching: true
  blame:
    defaultWidth: 300
    gutterFormat: "{date} {author}"
  core:
    audioBeep: false
    disabledPackages: [
      "relative-numbers"
      "linter-rubocop"
      "language-php"
      "language-perl"
      "language-objective-c"
      "language-mustache"
      "language-less"
      "language-java"
      "language-hyperlink"
      "language-gfm"
      "language-csharp"
      "language-clojure"
      "language-ember"
      "selection-mode"
    ]
    packagesWithKeymapsDisabled: [
      "ctrl-dir-scroll"
      "spell-check"
    ]
    packagesWithSnippetsDisabled: [
      "rails-snippets"
    ]
    telemetryConsent: "no"
    themes: [
      "pristine-ui"
      "base16-tomorrow-dark-theme"
    ]
    titleBar: "custom"
    useTreeSitterParsers: false
  "ctrl-dir-scroll": {}
  "custom-title":
    template: "<%= relativeFilePath %>"
  editor:
    autoIndentOnPaste: true
    backUpBeforeSaving: true
    fontSize: 15
    invisibles: {}
    normalizeIndentOnPaste: true
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "073e1332-ed7a-6bb3-7a3f-deb3c48bb2fe"
  "git-blame":
    columnWidth: 250
  "git-diff":
    showIconsInEditorGutter: true
  "linter-ui-default": {}
  livereload:
    applyCSSLive: false
  "markdown-preview":
    breakOnSingleNewline: true
  "markdown-table-formatter":
    markdownGrammarScopes: [
      "source.gfm"
      "text.plain.null-grammar"
    ]
  "rails-open-rspec": {}
  "relative-numbers":
    showAbsoluteNumbers: true
  rspec:
    command: "bundle exec rspec"
  tabs:
    addNewTabsAtEnd: true
    enableVcsColoring: true
  "tree-view":
    autoReveal: true
    focusOnReveal: false
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".basic.html.text":
  editor:
    normalizeIndentOnPaste: "true"
".erb.html.text":
  editor:
    normalizeIndentOnPaste: "true"
".git-commit.text":
  editor:
    normalizeIndentOnPaste: "true"
".git-config.source":
  editor:
    normalizeIndentOnPaste: "true"
".git-rebase.text":
  editor:
    normalizeIndentOnPaste: "true"
".js.regexp.replacement.source":
  editor:
    normalizeIndentOnPaste: "true"
".js.regexp.source":
  editor:
    normalizeIndentOnPaste: "true"
".js.source":
  editor:
    normalizeIndentOnPaste: "true"
".ruby.source":
  editor:
    normalizeIndentOnPaste: "true"
"file-types":
  hbs: "text.html.htmlbars"
"text.basic.html":
  editor:
    normalizeIndentOnPaste: "true"
